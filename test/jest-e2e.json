{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@dataclouder/dc-mongo": "<rootDir>/../libs/dc-mongo/src",
    "@dataclouder/dc-mongo/(.*)": "<rootDir>/../libs/dc-mongo/src/$1"
  }
}